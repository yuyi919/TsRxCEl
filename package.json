{
  "name": "tsrxcel",
  "version": "1.0.0",
  "private": true,
  "main": "build/main.js",
  "author": {
    "name": "yuyi",
    "email": "yuyi919606952@foxmail.com"
  },
  "homepage": "./",
  "description": "any",
  "sideEffects": false,
  "scripts": {
    "development": "cross-env NODE_ENV=development",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 scripts/start-electron.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron .",
    "electron:tsc": "tsc --project src/tsconfig.el.json",
    "electron:tsc2": "tsc -target es5 -removeComments true -lib dom,DOM.Iterable,es2015 -moduleResolution node -outDir build -rootDir src -baseUrl . -sourceMap -allowJs -emitDecoratorMetadata true -experimentalDecorators true -allowUnreachableCode src/main.ts",
    "start": "node scripts/start.js",
    "dev": "npm run electron:tsc && npm run hot-server",
    "dev2": "webpack-cli --config scripts/build-lib.js && npm run hot-server",
    "electron:linux": "npm run electron:tsc && npx electron-builder build --linux",
    "electron:windows": "npm run electron:tsc && npm run build:lib && npx electron-builder build --windows --x64",
    "electron:mac": "npm run electron:tsc && npx electron-builder build --mac",
    "build": "node scripts/build.js && npm run electron:tsc",
    "build:dll": "webpack-cli --config scripts/build-dll.js",
    "build:dll2": "cross-env DEV=true webpack-cli --config scripts/build-dll.js",
    "build:lib": "webpack-cli --config scripts/build-lib.js",
    "build:lib2": "cross-env ONLY=true webpack-cli --config scripts/build-lib.js",
    "build:main": "node scripts/build-main.js",
    "build:main2": "cross-env ONLY=true node scripts/build-main.js",
    "build:mainTS": "tsc --project src/main/tsconfig.main.json ",
    "build:win": "npm run build:main && npm run electron:windows && npm run asar",
    "build:win2": "npm run build:dll && npm run build:main && npm run electron:windows",
    "test": "node scripts/test.js",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "asar": "asar extract dist/win-unpacked/resources/app.asar dist/app"
  },
  "build": {
    "appId": "com.tsrxce.app",
    "files": [
      "build/**/*",
      "!**/*.map",
      "!**/node_modules/**/*",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "apk"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true,
      "installerIcon": "./build/favicon.ico",
      "uninstallerIcon": "./build/favicon.ico"
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.14",
    "es6-promise": "^4.2.5",
    "immer": "^1.8.2",
    "log4js-extend": "^0.2.1",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "react-hot-loader": "^4.3.12",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-soundplayer": "^1.0.4",
    "rxjs": "^6.3.3",
    "styled-components": "^3.4.10",
    "superagent": "^3.8.3",
    "superagent-charset": "^1.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/deepmerge": "^2.2.0",
    "@types/echarts": "0.0.13",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.14",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-loadable": "^5.4.2",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/superagent": "^3.8.5",
    "@types/xmldom": "^0.1.29",
    "add-asset-html-webpack-plugin": "^3.1.2",
    "ajv": "^6.6.1",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "7.1.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^6.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "deepmerge": "^2.2.1",
    "devtron": "^1.4.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron": "^2.0.14",
    "electron-builder": "^20.38.3",
    "electron-debug": "^2.0.0",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.4.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "fast-deep-equal": "^2.0.1",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "iconv-lite": "^0.4.24",
    "is-electron": "^2.2.0",
    "is-electron-renderer": "^2.0.1",
    "jest": "22.4.2",
    "jest-pnp-resolver": "^1.0.2",
    "jest-resolve": "^23.6.0",
    "log4js": "^3.0.6",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.2.1",
    "postcss-flexbugs-fixes": "3.2.0",
