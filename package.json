{
  "name": "tsrxcel",
  "version": "1.0.0",
  "private": true,
  "main": "build/main.js",
  "author": {
    "name": "yuyi",
    "email": "yuyi919606952@foxmail.com"
  },
  "homepage": "./",
  "description": "any",
  "scripts": {
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 scripts/start-electron.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron .",
    "electron:tsc": "tsc -target es5 -lib dom,DOM.Iterable,es2015 -moduleResolution node -outDir build -rootDir src -baseUrl . -sourceMap -allowJs -emitDecoratorMetadata true -experimentalDecorators true -allowUnreachableCode src/main.ts",
    "start": "node scripts/start.js",
    "dev": "npm run electron:tsc && npm run hot-server",
    "build": "node scripts/build.js && npm run electron:tsc",
    "build-main": "node scripts/build-main.js",
    "electron:linux": "npm run electron:tsc && npx electron-builder build --linux",
    "electron:windows": "npm run electron:tsc && npx electron-builder build --windows --x64",
    "electron:mac": "npm run electron:tsc && npx electron-builder build --mac",
    "build:win": "npm run build-main && npm run electron:windows",
    "test": "node scripts/test.js --env=jsdom"
  },
  "build": {
    "appId": "com.tsrxce.app",
    "files": [
      "build/**/*",
      "!**/*.map",
      "!**/node_modules/**/*",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "apk"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "runAfterFinish": true,
      "installerIcon": "./build/favicon.ico",
      "uninstallerIcon": "./build/favicon.ico"
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "fs-extra": "3.0.1",
    "deepmerge": "^2.2.1",
    "es6-promise": "^4.2.5",
    "iconv-lite": "^0.4.24",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "reflect": "^0.1.3",
    "reflect-metadata": "^0.1.12",
    "superagent": "^3.8.3",
    "superagent-charset": "^1.2.0",
    "@types/classnames": "^2.2.6",
    "@types/deepmerge": "^2.2.0",
    "@types/echarts": "0.0.13",
    "@types/electron": "^1.6.10",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-loadable": "^5.4.2",
    "@types/react-router": "^4.4.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/reflect-metadata": "^0.1.0",
    "@types/superagent": "^3.8.5",
    "@types/xmldom": "^0.1.29",
    "ajv": "^6.6.1",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.7",
    "devtron": "^1.4.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.14",
    "electron": "^2.0.14",
    "electron-builder": "^20.38.3",
    "electron-debug": "^2.0.0",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fast-deep-equal": "^2.0.1",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "html-webpack-plugin": "2.29.0",
    "immer": "^1.8.0",
    "jest": "22.4.2",
    "mobx": "^5.7.0",
    "mobx-react": "^5.4.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "raf": "3.4.0",
    "resolve": "1.6.0",
    "react": "^16.6.3",
    "react-dev-utils": "^5.0.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-soundplayer": "^1.0.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.19.0",
    "styled-components": "^3.4.10",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "D:\\WorkSpace\\TsRxCEl\\tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "plugins": [
    "react-hot-loader/babel"
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
}
